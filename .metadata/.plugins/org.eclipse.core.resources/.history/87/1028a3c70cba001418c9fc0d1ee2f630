		// TODO Auto-generated method stub

	}

	@Override
	public void newDayEvent(TimeEvent e) {
		// TODO Auto-generated method stub

	}

	@Override
	public Rectangle getBoundingBox() {
		return new Rectangle(x, y, x+TEXTURE_SIZE, y+TEXTURE_SIZE);
	}

	@Override
	public void move(int x, int i) {
		// TODO Auto-generated method stub
		
	}

}
